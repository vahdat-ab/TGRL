//	'urnspec' name= ID '{'
//		(
//		(urndef=URNdefinition)&
//		(grlspec=GRLspec)?&
//		(metadata+=Metadata*)&
//		(ucmspec=UCMspec)?&
//		(urnLinks+=URNlink*)&
//		(asdspec=ASDspec)
//		)
//	'}'
//	
