//		'indicatorGroup' name=ID '{'
//		(
//			('name' '=' label = STRING ';')?&
//			('description' '=' description=STRING ';')?&
//			('isRedesignCategory' '=' isRedesignCategory=BOOLEAN ';')?&
//			('indicators''=' indicators+=[Indicator](',' indicators+=[Indicator] )* ';')?
//		)
//	'}'
